/**
 * Exclusive-or gate:
 * out = not (a == b)
 */

CHIP Xor {
    IN a, b;
    OUT out;

    PARTS:
    // Put your code here:
    Not(in=a, out=NotAOut);
    Not(in=b, out=NotBOut);
    Nand(a=a, b=NotBOut, out=NandAOut);
    Nand(a=b, b=NotAOut, out=NandBOut);
    Nand(a=NandAOut, b=NandBOut, out=out);
}